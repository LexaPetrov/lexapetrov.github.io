{
  "name": "lexapetrov.github.io",
  "version": "0.1.0",
  "dependencies": {
    "cp-cli": "^2.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "predeploy": "npm run build",
